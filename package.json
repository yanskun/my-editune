{
  "name": "my-editune",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "nuxt-ts",
    "build": "nuxt-ts build",
    "generate": "nuxt-ts generate",
    "generate:staging": "cross-env NODE_ENV=staging nuxt-ts generate",
    "start": "nuxt-ts start",
    "lint:ts": "eslint --ext \".ts,.js,.vue\" --ignore-path .gitignore .",
    "lint:style": "stylelint \"**/*.{vue,css}\" --ignore-path .gitignore",
    "lint:fix": "eslint --fix --ext \".ts,.js,.vue\" --ignore-path .gitignore . && stylelint --fix \"**/*.{vue,css}\" --ignore-path .gitignore",
    "lint": "yarn lint:ts && yarn lint:style",
    "storybook": "start-storybook -p 6006",
    "build:storybook": "yarn build-storybook -o ./docs",
    "snapshot": "jest --updateSnapshot",
    "hygen": "hygen generate component",
    "prepare": "husky install",
    "pre-commit-lint": "yarn check-types && yarn lint-staged"
  },
  "dependencies": {
    "@nuxt/content": "^1.15.0",
    "@nuxt/typescript-runtime": "^2.1.0",
    "@nuxtjs/axios": "^5.13.6",
    "@nuxtjs/google-gtag": "^1.0.4",
    "@nuxtjs/pwa": "^3.3.5",
    "@vue/composition-api": "^1.4.0",
    "add": "^2.0.6",
    "core-js": "^3.19.1",
    "cross-env": "^7.0.3",
    "element-ui": "^2.15.6",
    "nuxt": "^2.15.8",
    "vue-clipboard3": "^1.0.1",
    "vue-highlightjs": "^1.3.3",
    "yarn": "^1.22.17"
  },
  "devDependencies": {
    "@commitlint/cli": "^14.1.0",
    "@commitlint/config-conventional": "^14.1.0",
    "@nuxt/types": "^2.15.8",
    "@nuxt/typescript-build": "^2.1.0",
    "@nuxtjs/eslint-config": "^7.0.0",
    "@nuxtjs/eslint-config-typescript": "^7.0.2",
    "@nuxtjs/eslint-module": "^3.0.2",
    "@nuxtjs/stylelint-module": "^4.0.0",
    "@storybook/addon-actions": "^6.3.12",
    "@storybook/addon-essentials": "^6.3.12",
    "@storybook/addon-knobs": "^6.2.9",
    "@storybook/addon-links": "^6.3.12",
    "@storybook/vue": "^6.3.12",
    "@vue/test-utils": "^1.2.2",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^27.3.1",
    "babel-loader": "^8.2.3",
    "css-loader": "^6.5.1",
    "eslint": "^8.2.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-nuxt": "^3.0.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.0.1",
    "husky": "^7.0.4",
    "hygen": "^6.1.0",
    "jest": "^26.6.3",
    "lint-staged": "^12.0.2",
    "node-sass": "^4.14.1",
    "prettier": "^2.4.1",
    "pug": "^3.0.2",
    "pug-plain-loader": "^1.1.0",
    "sass-loader": "^10.1.1",
    "storybook": "^6.3.12",
    "stylelint": "^13.13.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^22.0.0",
    "vue-jest": "^3.0.4",
    "vue-loader": "^15.9.8",
    "vue-template-compiler": "^2.6.12",
    "vuex-module-decorators": "^1.0.1"
  },
  "engines": {
    "npm": "please_use_yarn_instead"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": "plugin:vue/recommended",
    "parserOptions": {
      "parser": "babel-eslint"
    }
  }
}
