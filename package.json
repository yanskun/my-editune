{
  "name": "my-editune",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "nuxt-ts",
    "build": "nuxt-ts build",
    "generate": "nuxt-ts generate",
    "start": "nuxt-ts start",
    "lint:ts": "eslint --ext \".ts,.js,.vue\" --ignore-path .gitignore .",
    "lint:style": "stylelint \"**/*.{vue,css}\" --ignore-path .gitignore",
    "lint:fix": "eslint --fix --ext \".ts,.js,.vue\" --ignore-path .gitignore . && stylelint --fix \"**/*.{vue,css}\" --ignore-path .gitignore",
    "lint": "yarn lint:ts && yarn lint:style",
    "test": "jest",
    "storybook": "nuxt-ts storybook -p 6006",
    "build:storybook": "nuxt storybook build -o ./docs",
    "snapshot": "jest --updateSnapshot",
    "hygen": "hygen generate component",
    "prepare": "husky install",
    "pre-commit-lint": "yarn check-types && yarn lint-staged"
  },
  "dependencies": {
    "@nuxt/content": "^1.14.0",
    "@nuxt/typescript-runtime": "^2.1.0",
    "@nuxtjs/axios": "^5.13.6",
    "@nuxtjs/pwa": "^3.3.5",
    "core-js": "^3.14.0",
    "element-ui": "^2.15.2",
    "nuxt": "^2.15.4",
    "postcss": "^8.3.0",
    "vue-highlightjs": "^1.3.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@nuxt/types": "^2.15.6",
    "@nuxt/typescript-build": "^2.1.0",
    "@nuxtjs/eslint-config": "^6.0.0",
    "@nuxtjs/eslint-config-typescript": "^6.0.1",
    "@nuxtjs/eslint-module": "^3.0.2",
    "@nuxtjs/storybook": "^4.0.4",
    "@nuxtjs/stylelint-module": "^4.0.0",
    "@storybook/addon-actions": "^6.2.9",
    "@storybook/addon-essentials": "^6.2.7",
    "@storybook/addon-knobs": "^6.2.9",
    "@storybook/addon-links": "^6.2.9",
    "@vue/test-utils": "^1.1.4",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^27.0.2",
    "babel-loader": "^8.2.2",
    "css-loader": "^5.2.6",
    "eslint": "^7.28.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-nuxt": "^2.0.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-vue": "^7.10.0",
    "husky": "^6.0.0",
    "hygen": "^6.1.0",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.3",
    "node-sass": "^4.14.1",
    "prettier": "^2.3.1",
    "pug": "^3.0.2",
    "pug-plain-loader": "^1.1.0",
    "sass-loader": "^10.1.1",
    "storybook": "^6.3.0-rc.0",
    "stylelint": "^13.13.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^22.0.0",
    "vue-jest": "^3.0.4",
    "vue-loader": "^15.9.6",
    "vue-template-compiler": "^2.6.12",
    "vuex-module-decorators": "^1.0.1"
  },
  "engines": {
    "npm": "please_use_yarn_instead"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": "plugin:vue/recommended",
    "parserOptions": {
      "parser": "babel-eslint"
    }
  }
}
